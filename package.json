{
	"name": "groq-code-cli",
	"version": "1.0.2",
	"license": "MIT",
	"bin": {
		"groq": "dist/core/cli.js"
	},
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "rm -rf dist && tsc",
		"dev": "tsc --watch",
		"test": "ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"chalk": "^5.4.1",
		"commander": "^14.0.0",
		"groq-sdk": "^0.27.0",
		"https-proxy-agent": "^7.0.6",
		"meow": "^11.0.0",
		"socks-proxy-agent": "^8.0.5"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"xo": "^1.2.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		],
		"files": [
			"src/**/*.test.ts",
			"!**/*.d.ts"
		]
	},
	"xo": {
		"prettier": true
	},
	"prettier": "@vdemedes/prettier-config"
}
